<Window x:Class="TossalERP.CreacionMusico"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TossalERP"
        mc:Ignorable="d"
        Title="Creacion músico" Height="450" Width="400" Closed="Window_Closed">
    <Grid>
        <StackPanel Margin="10,10,10,10">
            <TextBlock>Nombre</TextBlock>
            <TextBox></TextBox>
            <TextBlock>Instrumento</TextBlock>
            <ComboBox x:Name="Instrumentos" ></ComboBox>
        </StackPanel>
        
    </Grid>
</Window>
